declareInfix(=>, 500)	# or what?

?f?app(?x) == apply(?f, ?x)

apply(?f?symbol, ?value) == ?f(?value)
apply(?var => ?expr, ?value) == subst(?var, ?value, ?expr)

subst(?var, ?value, ?x) == ?x
subst(?var, ?value, ?var) == ?value
subst(?var, ?value, ?f()) == make_app(subst(?var, ?value, ?f))
subst(?var, ?value, ?f(?x)) == make_app(subst(?var, ?value, ?f), subst(?var, ?value, ?x))
subst(?var, ?value, ?f(?x, ?y)) == make_app(subst(?var, ?value, ?f), subst(?var, ?value, ?x), subst(?var, ?value, ?y))
subst(?var, ?value, ?f(?x, ?y, ?z)) == make_app(subst(?var, ?value, ?f), subst(?var, ?value, ?x), subst(?var, ?value, ?y), subst(?var, ?value, ?z))
subst(?var, ?value, ?var => ?expr) == ?var => ?expr

make_app(?f) == ?f()
make_app(?f, ?x) == ?f(?x)
make_app(?f, ?x, ?y) == ?f(?x, ?y)
make_app(?f, ?x, ?y, ?z) == ?f(?x, ?y, ?z)

